    "# first get all lines from file\n",
    "with open('Equation_unimib_10.txt', 'r') as f:\n",
    "    lines = f.readlines()\n",
    "\n",
    "# remove spaces\n",
    "lines = [line.replace(' ', '') for line in lines]\n",
    "\n",
    "# finally, write lines in the file\n",
    "with open('Equation_unimib_10.txt', 'w') as f:\n",
    "    f.writelines(lines)"
